{
    "name": "justinrainbow/epub",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Rainbow",
            "email": "justin.rainbow@gmail.com"
        },
        {
            "name": "Sam Bedwell",
            "email": "sam.bedwell@lurn.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/collections": "^8.0",
        "illuminate/support": "^8.0",
        "spatie/data-transfer-object": "^2.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.13",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Lurn\\EPub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lurn\\EPub\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
